{
  "name": "ai-development-assistant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "vite",
    "dev:backend": "node scripts/start-backend.js",
    "build": "tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:backend": "cd backend && npm run build",
    "start:backend": "node scripts/start-backend.js",
    "start:full": "concurrently \"npm run start:backend\" \"npm run dev\"",
    "start": "npm run start:full",
    "install:all": "npm install && cd backend && npm install",
    "check:backend": "curl -s http://localhost:3001/health || echo 'Backend server is not running'"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@reactflow/background": "^11.0.0",
    "@reactflow/controls": "^11.0.0",
    "@reactflow/core": "^11.0.0",
    "@reactflow/minimap": "^11.0.0",
    "@supabase/supabase-js": "^2.50.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.263.1",
    "pg": "^8.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.60.0",
    "react-hot-toast": "^2.5.2",
    "react-resizable-panels": "^2.0.0",
    "react-router-dom": "^7.6.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "@cloudflare/workers-types": "^4.20250709.0",
    "@hono/node-server": "^1.15.0",
    "@types/node": "24.0.11",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.2.0",
    "dotenv": "^17.1.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "hono": "^4.8.4",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2",
    "typescript-eslint": "^8.36.0",
    "vite": "^4.4.5",
    "wrangler": "^4.24.0"
  }
}
